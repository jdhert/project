  const script = document.createElement('script');\n    script.type = 'text/javascript';\n    script.src = '//dapi.kakao.com/v2/maps/sdk.js?appkey=c2a63c53b4bb9f45634c727367987e63';\n    script.onload = this.initMap; // 스크립트 로드 완료 후 맵 초기화 함수 호출\n    document.head.appendChild(script);\n  },\n  methods: {\n    initMap() {\n\t\tif (typeof kakao === 'undefined' || !kakao.maps) {\n    console.error('Kakao 지도 API를 불러올 수 없습니다.');\n    return;\n  }\n      \n      // Kakao 지도 API를 사용하여 지도를 초기화하는 코드\n      const container = document.getElementById('map');\n\t  const options = {\n \t\tcenter: new kakao.maps.LatLng(37.566826, 126.9786567), // 서울시청을 중심으로 설정\n  \t\tlevel: 3 // 지도 확대 레벨\n\t};\n\n      // 지도에 마커 추가\n\t// eslint-disable-next-line\n      const markerPosition = new kakao.maps.LatLng(37.566826, 126.9786567); // 서울시청 위치\n     // eslint-disable-next-line \n\t  const marker = new kakao.maps.Marker({\n        position: markerPosition\n      });\n\t// eslint-disable-next-line\n      const map = new kakao.maps.Map(container, options);\n      marker.setMap(map);\n    }\n  }\n}\n</script>\n\n\n<style>\n@import '../assets/css/skel.css';\n@import '../assets/css/style.css';\n@import '../assets/css/style-xlarge.css';\n\n@font-face { \n  font-family: \"continuous\";\n  src: url(\"../assets/fonts/continuous.ttf\") format(\"truetype\");\n  font-weight: normal;\n}\n</style>"],"mappings":"AA8HA,eAAe;EACbA,IAAI,EAAE,YAAY;EAClBC,KAAK,EAAE;IACLC,GAAG,EAAEC;EACP,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR;IACA,MAAMC,MAAK,GAAIC,QAAQ,CAACC,aAAa,CAAC,QAAQ,CAAC;IAC/CF,MAAM,CAACG,IAAG,GAAI,iBAAiB;IAC/BH,MAAM,CAACI,GAAE,GAAI,yEAAyE;IACtFJ,MAAM,CAACK,MAAK,GAAI,IAAI,CAACC,OAAO,EAAE;IAC9BL,QAAQ,CAACM,IAAI,CAACC,WAAW,CAACR,MAAM,CAAC;EACnC,CAAC;EACDS,OAAO,EAAE;IACPH,OAAOA,CAAA,EAAG;MACZ,IAAI,OAAOI,KAAI,KAAM,WAAU,IAAK,CAACA,KAAK,CAACC,IAAI,EAAE;QAC/CC,OAAO,CAACC,KAAK,CAAC,2BAA2B,CAAC;QAC1C;MACF;;MAEI;MACA,MAAMC,SAAQ,GAAIb,QAAQ,CAACc,cAAc,CAAC,KAAK,CAAC;MACnD,MAAMC,OAAM,GAAI;QAChBC,MAAM,EAAE,IAAIP,KAAK,CAACC,IAAI,